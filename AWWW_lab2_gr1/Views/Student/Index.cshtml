@model AWWW_lab1_gr1.Models.Student

<h2 class="page-title">Studenci</h2>

<p>Studenci:</p>

<ul>

    @for (int i = 0; i < @ViewBag.TotalStudents; i++)
    {
    <li>
        <a asp-controller="student" asp-action="Students" asp-route-id="@ViewBag.students[i].Id">@ViewBag.students[i].FirstName @ViewBag.students[i].SecondName</a>
    </li>
    }

</ul>